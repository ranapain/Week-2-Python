def multiply_poly(p1, p2):
    n, m = len(p1), len(p2)
    result = [0] * (n + m - 1)

    for i in range(n):
        for j in range(m):
            result[i + j] += p1[i] * p2[j]

    return result

# Example: (3x² + 5x + 6) × (6x² + 8x + 4)
p1 = [3, 5, 6]   # 3x² + 5x + 6
p2 = [6, 8, 4]   # 6x² + 8x + 4

print(multiply_poly(p1, p2))
